<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Alta</servlet-name>
        <servlet-class>conexiones.Alta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>conexiones.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NuevaMina</servlet-name>
        <servlet-class>conexiones.NuevaMina</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CerrarSesion</servlet-name>
        <servlet-class>conexiones.CerrarSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MejorarMina</servlet-name>
        <servlet-class>conexiones.MejorarMina</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NuevoCampamento</servlet-name>
        <servlet-class>conexiones.NuevoCampamento</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MejorarAtaque</servlet-name>
        <servlet-class>conexiones.MejorarAtaque</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MejorarDefensa</servlet-name>
        <servlet-class>conexiones.MejorarDefensa</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ComprarTropasAtaque</servlet-name>
        <servlet-class>conexiones.ComprarTropasAtaque</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Alta</servlet-name>
        <url-pattern>/Alta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NuevaMina</servlet-name>
        <url-pattern>/NuevaMina</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CerrarSesion</servlet-name>
        <url-pattern>/CerrarSesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MejorarMina</servlet-name>
        <url-pattern>/MejorarMina</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NuevoCampamento</servlet-name>
        <url-pattern>/NuevoCampamento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MejorarAtaque</servlet-name>
        <url-pattern>/MejorarAtaque</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MejorarDefensa</servlet-name>
        <url-pattern>/MejorarDefensa</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ComprarTropasAtaque</servlet-name>
        <url-pattern>/ComprarTropasAtaque</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
